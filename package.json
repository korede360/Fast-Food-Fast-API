    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^2.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.22.2",
    "mongoose": "^5.2.10",
    "nodemailer": "^4.6.8",
    "pg": "^7.4.3",
    "pg-promise": "^8.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
