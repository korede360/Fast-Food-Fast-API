{
  "name": "fast-food-fast-api",
  "version": "1.0.0",
  "description": "Simple food ordering app.",
  "main": "src/app.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "clean": "rm -rf ./dist",
    "build": "npx babel src -d dist --source-maps",
    "test": "nyc --reporter=lcov --reporter=text mocha --require @babel/register --exit",
    "dev": "nodemon --exec npx babel-node ./src/app.js",
    "heroku-postbuild": "npm run build",
    "start": "node dist/app"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ]
  },
  "keywords": [],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^8.2.6",
